@model List<AnagDTO>
@{
    Layout = null;
}
@if (Model?.Count() > 0)
{
    foreach (var dto in Model)
    {
        <div class="col-12 col-md-4 col-lg-3 p-3">
            <partial name="~/Areas/Admin/Views/Shared/_AnagCard.cshtml" model="dto" />
        </div>
    }
}
else
{
    <div class="col-12">
        <h2>No Data</h2>
    </div>
}